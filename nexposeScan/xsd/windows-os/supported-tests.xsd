<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <xsd:simpleType name="test_type">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="equals"/>
      <xsd:enumeration value="notEquals"/>
      <xsd:enumeration value="keyIsPresent"/>
      <xsd:enumeration value="keyNotPresent"/>
      <xsd:enumeration value="valueIsPresent"/>
      <xsd:enumeration value="valueNotPresent"/>
      <xsd:enumeration value="match"/>
      <xsd:enumeration value="find"/>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
